    <AssemblyName>Aranasoft.Cobweb.EntityFrameworkCore.Validation</AssemblyName>
    <RootNamespace>Aranasoft.Cobweb.EntityFrameworkCore.Validation</RootNamespace>
    <VersionPrefix>1.20.0</VersionPrefix>
