<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\..\..\netfx.props" />

  <PropertyGroup>
    <TargetFrameworks>net40</TargetFrameworks>
    <AssemblyName>Aranasoft.Cobweb.Mvc</AssemblyName>
    <RootNamespace>Aranasoft.Cobweb.Mvc</RootNamespace>
    <VersionPrefix>1.40.5</VersionPrefix>
  </PropertyGroup>

  <PropertyGroup>
    <Description>Boilerplate components for testable and convenient ASP.NET MVC development.</Description>
    <PackageTags>aspnet;mvc;$(PackageTags)</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.Mvc" Version="[4.0.40804,5)" />
    <PackageReference Include="Newtonsoft.Json" Version="[4.5.11,)" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Web" />
    <Reference Include="System.Web.ApplicationServices" />
  </ItemGroup>

</Project>

