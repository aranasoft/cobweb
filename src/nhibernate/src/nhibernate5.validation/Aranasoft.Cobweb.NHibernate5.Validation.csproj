    <ItemGroup>
        <PackageReference Include="Aranasoft.Cobweb" Version="[1.4,2)" />
        <PackageReference Include="Aranasoft.Cobweb.NHibernate" Version="[1.50.3,1.60)" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\..\cobweb\src\Cobweb\Aranasoft.Cobweb.csproj" />
