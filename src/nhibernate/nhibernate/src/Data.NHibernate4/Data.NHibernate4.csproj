      <Compile Include="..\Data.NHibernate3\INHibernateFactoryBuilder.cs" Link="INHibernateFactoryBuilder.cs" />
      <Compile Include="..\Data.NHibernate3\INHibernateSessionBuilder.cs" Link="INHibernateSessionBuilder.cs" />
      <Compile Include="..\Data.NHibernate3\NHibernateTransactionHandler.cs" Link="NHibernateTransactionHandler.cs" />
      <Compile Include="..\Data.NHibernate3\Providers\EagerFetch.cs" Link="Providers\EagerFetch.cs" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Cobweb" Version="[1.1.1,2)" />
        <PackageReference Include="FluentNHibernate" Version="[2,2.1)" />
    </ItemGroup>

</Project>