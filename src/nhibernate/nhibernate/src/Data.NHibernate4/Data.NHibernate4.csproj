<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="../../Common.props" />

    <PropertyGroup>
        <TargetFramework>net45</TargetFramework>
        <AssemblyName>Cobweb.Data.NHibernate</AssemblyName>
        <RootNamespace>Cobweb.Data.NHibernate</RootNamespace>
        <Version>1.40.3</Version>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
      <Compile Include="..\Data.NHibernate3\EntityTypeSource.cs" Link="EntityTypeSource.cs" />
